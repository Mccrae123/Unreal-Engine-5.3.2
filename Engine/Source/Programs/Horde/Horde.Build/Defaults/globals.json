			"condition": "true"
		},
		{
			"id": "win64",
			"name": "Win64",
			"condition": "Platform == 'Win64'"
		},
		{
			"id": "mac",
			"name": "Mac",
			"condition": "Platform == 'Mac'"
		},
		{
			"id": "linux",
			"name": "Linux",
			"condition": "Platform == 'Linux'"
		}
