	OutTexture[uint2(BaseU + 12, BaseV)] = float4(RGB12, 1.0);
	OutTexture[uint2(BaseU + 13, BaseV)] = float4(RGB13, 1.0);
	OutTexture[uint2(BaseU + 14, BaseV)] = float4(RGB14, 1.0);
